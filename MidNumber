package com.nextgen.javaexamples;

public class MidNumber {
	public static void main(String args[])
	{
		int a = 1;
		int b = 5;
		int c = 2;
		if(a>b)
		{
			if(a<c)
			{
				System.out.println("A");
			}
			else
			{
				System.out.println("B");
			}
		}
		else if (b>c)
		{
			if (a<b)
				System.out.println("A");
			else
				System.out.println("c");
		}
	}

}
