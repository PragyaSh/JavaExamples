package com.nextgen.javaexamples;

public class Fibonacci {
	public static void main(String args[])
	{
		int f1 = 0;
		int f2 = 1;
		int n = 6;
		int fibSum = 0;
		for (int i = 0; i < n-2; i++)
		{
			fibSum = f1 + f2;
			f1 = f2;
			f2 = fibSum;
		}
			System.out.println("Fibonacci Sum " + fibSum);	
	}
}
